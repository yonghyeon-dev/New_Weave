{
  "name": "weave-app",
  "version": "1.3.0",
  "description": "Weave - 업무 관리 통합 플랫폼",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:a11y": "vitest --config vitest.config.a11y.ts",
    "test:security": "vitest --config vitest.config.security.ts",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf .next out dist",
    "analyze": "cross-env ANALYZE=true next build",
    "pre-deploy": "node scripts/pre-deployment-check.js",
    "deploy:staging": "npm run pre-deploy && vercel --env=preview",
    "deploy:production": "npm run pre-deploy && vercel --prod"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.32",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest-axe": "^10.0.0",
    "next": "^14.2.32",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hello-pangea/dnd": "^18.0.1",
